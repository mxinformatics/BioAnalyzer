{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "researchApi": {
      "type": "project.v0",
      "path": "../BioAnalyzer.Research.Api/BioAnalyzer.Research.Api.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{researchApi.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "researchApp": {
      "type": "container.v1",
      "build": {
        "context": "../BioAnalyzer.App",
        "dockerfile": "../BioAnalyzer.App/Dockerfile"
      },
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{researchApp.bindings.http.targetPort}",
        "services__researchApi__http__0": "{researchApi.bindings.http.url}",
        "services__researchApi__https__0": "{researchApi.bindings.https.url}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8000,
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8001,
          "external": true
        }
      }
    },
    "eventHandlers": {
      "type": "container.v1",
      "build": {
        "context": "../BioAnalyzer.EventHandlers",
        "dockerfile": "../BioAnalyzer.EventHandlers/Dockerfile"
      },
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated",
        "AzureFunctionsJobHost__telemetryMode": "OpenTelemetry",
        "ASPNETCORE_URLS": "http://\u002B:{eventHandlers.bindings.http.targetPort}",
        "AzureWebJobsStorage__blobServiceUri": "{funcstoragecfe1f.outputs.blobEndpoint}",
        "AzureWebJobsStorage__queueServiceUri": "{funcstoragecfe1f.outputs.queueEndpoint}",
        "AzureWebJobsStorage__tableServiceUri": "{funcstoragecfe1f.outputs.tableEndpoint}",
        "Aspire__Azure__Storage__Blobs__AzureWebJobsStorage__ServiceUri": "{funcstoragecfe1f.outputs.blobEndpoint}",
        "Aspire__Azure__Storage__Queues__AzureWebJobsStorage__ServiceUri": "{funcstoragecfe1f.outputs.queueEndpoint}",
        "Aspire__Azure__Data__Tables__AzureWebJobsStorage__ServiceUri": "{funcstoragecfe1f.outputs.tableEndpoint}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8080
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8080
        }
      }
    },
    "funcstoragecfe1f": {
      "type": "azure.bicep.v0",
      "path": "funcstoragecfe1f.module.bicep"
    }
  }
}